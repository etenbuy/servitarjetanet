  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Cantidadsolicitada">
          <DataField>Cantidadsolicitada</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Monto_Pagadosolicitada">
          <DataField>Monto_Pagadosolicitada</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DSServiTarjeta</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT Count(Monto_Pagado) as Cantidadsolicitada,
            sum(Monto_Pagado) as Monto_Pagadosolicitada
            FROM   Solicitud 
            WHERE  LoginCreado =@Mail
            AND  Numero_TDC =@TDC
            AND StatusSolicitudID = 1
            AND  MONTH(FechaCreado) = @Month
            AND YEAR(FechaCreado) = @Year
            

SELECT Count(Monto_Pagado) as Cantidadrechazada,
            sum(Monto_Pagado) as Monto_Pagadorechazada
            FROM   Solicitud 
             WHERE  LoginCreado =@Mail
            AND  Numero_TDC =@TDC
            AND StatusSolicitudID = 3
            AND  MONTH(FechaCreado) = @Month
            AND YEAR(FechaCreado) = @Year
            
            SELECT Count(Monto_Pagado) as Cantidadpagada,
            sum(Monto_Pagado) as Monto_Pagadopagada
            FROM   Solicitud 
       WHERE  LoginCreado =@Mail
            AND  Numero_TDC =@TDC
            AND StatusSolicitudID = 4
            AND  MONTH(FechaCreado) = @Month
            AND YEAR(FechaCreado) = @Year

SELECT top(1) Saldo             
            FROM   Solicitud 
         WHERE  LoginCreado =@Mail
            AND  Numero_TDC =@TDC
           AND  MONTH(FechaCreado) = @Month
            AND YEAR(FechaCreado) = @Year
            ORDER BY FechaCreado DESC;
            

SELECT FechaCreado as Fecha,
                 Descripcion,
                 SolicitudID as Nro,
                  Numero_Factura, 
               Monto, 
                Monto_Pagado, 
                 StatusSolicitudID, 
                 Saldo             
            FROM   Solicitud 
           WHERE  LoginCreado =@Mail
            AND  Numero_TDC =@TDC
            AND  MONTH(FechaCreado) = @Month
            AND YEAR(FechaCreado) = @Year
            ORDER BY FechaCreado;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@Mail">
            <Value>=Parameters!Mail.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TDC">
            <Value>=Parameters!TDC.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
